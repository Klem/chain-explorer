{{#if error}}
    {{> error}}
{{/if}}
{{#unless error}}
    {{#if this.length}}
        <table class="table table-bordered table-hover table-sm align-middle">
            <thead class="table-light">
            <tr>
                <th>Block</th>
                <th>Hash</th>
                <th>Value</th>
                <th>Asset</th>
                <th>Type</th>
                <th>Timestamp</th>
            </tr>
            </thead>
            <tbody>
            {{#each this}}
                <tr>
                    <td>{{blockNumber}}</td>
                    <td><a href="#" class="tx-hash-link">{{hash}}</a></td>
                    <td>{{value}}</td>
                    <td>{{asset}}</td>
                    <td>{{category}}</td>
                    <td>{{timestamp}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    {{else}}
        <div class="alert alert-info d-flex flex-column align-items-center text-center" role="alert">
            <strong>No Nfts to display</strong>
        </div>
    {{/if}}
{{/unless}}

